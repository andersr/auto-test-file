{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules/**/*.*", "tmp/**/*.*"]
  }
}
